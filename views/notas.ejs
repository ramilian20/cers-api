    <section class="formulario-notas">
      <div class="contenedor-formulario-notas">
        <div class="form-notas">
          <form method="POST">
            <div class="input-label">
              <label> cedula</label>
              <input
                class="input-formulario-notas"
                type="number"
                id="identificacion"
                name="identificacion"
                placeholder="cedula"
              />

              <input class="bton" type="submit" value="Consultar" />
            </div>
          </form>
        </div>
      </div>
    </section>

    <section class="notas">
      <div class="contenedor-notas-targetas">
        <div class="targeta">
          <% if (arrayNombresDB.length > 0) { %> <%
          arrayNombresDB.forEach(Nombres => { %>
          <div class="cabecera-targeta">
            <div class="datos-cabecera">
              <h1>Nombre: <%= Nombres.nombre %></h1>
            </div>
            <div class="datos-cabecera">
              <h1>Apellido: <%= Nombres.apellido %></h1>
            </div>
            <div class="datos-cabecera">
              <h1>Programa: <%= Nombres.programa %></h1>
            </div>
          </div>
          <div class="cabecera-targetados">
            <div class="datos-cabecera">
              <h1>Modulo</h1>
            </div>
            <div class="datos-cabecera">
              <h1>Nota</h1>
            </div>
          </div>
          <% }) %> <% } %> <% if (arrayNotasDB.length > 0) { %> <%
          NotasDB.forEach(Notas => { %>
          <div class="cuerpo-targeta">
            <div class="modulo-nota">
              <div class="datos-cuerpo">
                <h1><%= Notas.modulo %></h1>
              </div>
              <div class="datos-cuerpo">
                <h1><%= Notas.nota %></h1>
              </div>
            </div>
          </div>
          <% }) %> <% } %>
        </div>
      </div>
    </section>
